{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "outDir": "./dist/",
    "baseUrl": "src",
    "paths": {
      "unnamed-network/*" : ["*"]
    }
  }
}
