{
  "name": "unamed-network",
  "version": "0.0.1",
  "description": "IPFS and WebRTC p2p network",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist ./.cache ./.parcel-cache",
    "build": "parcel build index.html --no-scope-hoist",
    "serve": "parcel serve index.html -p 8888 --no-hmr",
    "start": "npm run clean && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pastleo/unamed-network.git"
  },
