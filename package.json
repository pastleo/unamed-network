{
  "name": "unamed-network",
  "version": "0.0.1",
  "description": "IPFS and WebRTC p2p network",
  "type": "module",
  "module": "lib/unamed-network.js",
  "types": "index.d.ts",
  "targets": {
    "module": false,
    "types": false,
    "webLib": {
      "source": "src/webLib.js",
      "context": "browser",
      "includeNodeModules": true,
      "isLibrary": true,
      "optimize": false,
      "outputFormat": "commonjs",
      "sourceMap": false,
      "distDir": "webLib"
    }
  },
  "browserslist": "Firefox ESR",
  "exports": {
    ".": "./lib/unamed-network.js",
    "./webLib": "./webLib/webLib.js"
  },
  "scripts": {
    "postinstall": "./scripts/patch-node-modules.sh",
    "clean": "rm -rf dist .cache .parcel-cache",
    "web": "npm run clean && parcel serve index.html -p 8888 --no-hmr",
    "build": "npm run clean && parcel build",
    "start": "node lib/node.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pastleo/unamed-network.git"
  },
  "author": "PastLeo",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/pastleo/unamed-network/issues"
  },
  "homepage": "https://github.com/pastleo/unamed-network",
  "peerDependencies": {
    "ipfs-core": "^0.12.1"
  },
  "devDependencies": {
    "ipfs-http-client": "^54.0.1",
    "libp2p-websockets": "^0.16.2",
    "parcel": "^2.0.1",
    "tsee": "^1.3.2"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "isomorphic-ws": "^4.0.1",
    "k-bucket": "^5.1.0",
    "libp2p-crypto": "^0.20.0",
    "libp2p-webrtc-peer": "^10.0.1",
    "multiaddr": "^10.0.1",
    "multiaddr-to-uri": "^8.0.0",
    "multiformats": "^9.4.10",
    "uint8arrays": "^3.0.0",
    "ws": "^8.3.0"
  }
}
